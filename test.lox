class A {
    a() {
        print "a in A";
    }
}

class B < A {
    a() {
        super.A.a();
        print "a in B";
    }
}

class C < A {
    a() {
        super.A.a();
        print "a in C";
    }
}

class D < B, C {
    a() {
        super.B.a();
        super.C.a();
        print "a in D";
    }
}

var d = D();
d.a();